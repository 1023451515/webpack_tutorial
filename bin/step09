npm install --save-dev babel-core --registry=https://registry.npm.taobao.org
npm install --save-dev babel-plugin-transform-runtime --registry=https://registry.npm.taobao.org
npm install --save-dev babel-preset-es2015 --registry=https://registry.npm.taobao.org
npm install --save-dev babel-runtime --registry=https://registry.npm.taobao.org
